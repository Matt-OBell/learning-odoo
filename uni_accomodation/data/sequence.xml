<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Accomodation-->
        <record id="seq_student_accomodation" model="ir.sequence">
            <field name="name">Accomodation Sequence</field>
            <field name="code">student.accomodation.sequence</field>
            <field name="prefix">STD/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
