<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="view_form_res_library" model="ir.ui.view">
            <field name="name">Res Library Member Form</field>
            <field name="model">res.library.member</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="student_id"/>
                                <field name="library_id"/>
                            </group>
                            <group>
                                <field name="date_of_membership"/>
                                <field name="total_books_checkout"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_tree_res_library" model="ir.ui.view">
            <field name="name">Res Library Member Tree</field>
            <field name="model">res.library.member</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="student_id"/>
                    <field name="library_id"/>
                    <field name="date_of_membership"/>
                    <field name="total_books_checkout"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>